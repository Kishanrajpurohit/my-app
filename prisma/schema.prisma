
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  relationMode = "prisma"
}
model customers {
  id Int @id @default(autoincrement())
 
  customername String
  mobilenumber String
  devicename String
  problem   String
  status    String
}